set number
set nocompatible
set nowrap
set ignorecase
set smartcase
set showcmd
set showmode
set relativenumber
set cursorline

syntax on

set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx

filetype on

set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab

set clipboard=unnamedplus
colorscheme quiet
